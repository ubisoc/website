<meta property="og:title" content="<%= (current_page.data.htitle ? current_page.data.htitle + " | " : "") + "University Of Birmingham Islamic Society" %>" />
<meta property="og:type" content="article" />
<meta property="og:url" content="http://ubisoc.co.uk/" />
<meta property="og:image" content="http://ubisoc.co.uk/images/logo.png" />
<meta property="og:description" content="<%= 'The University of Birmingham Islamic Society.' + (current_page.data.description ? ' ' + current_page.data.description : '') %>" />
<meta property="og:site_name" content="The University of Birmingham Islamic Society" />

